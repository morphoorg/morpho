{
    "stan": {
        
        "name": "MH_analyzer",
        
        "model":   {
            "file": "./examples/MassHierarchy/models/MH_analysis.stan",
            "function_file": [
                              {"name" : "func_routines", "file" : "./functions/func_routines.functions"},
                              {"name" : "MH_Functions", "file" : "./functions/MH_functions.functions"}
                              ],
            "cache": "./examples/MassHierarchy/cache"
        },
        
        "data": {
            "type": "mc",
            "files": [
                      {
                      "name": "./examples/MassHierarchy/data/MH_analyze.data",
                      "format":"R"
                      },
                      {
                      "name" : "./examples/MassHierarchy/results/MHtest_generator",
                      "format" : "hdf5",
                      "event_number" : 0,
                      "datasets" : [
                                    {"nm" : "KEout", "stan_alias" : "KEin"},
                                    {"nm" : "Nout", "stan_alias" : "N", "data_format": "integer"}
                                    ]
                      }

                      ],
            "parameters": [
                           {
                           "chainnum" : 3,
                           "iternum" : 3000,
                           "nEvents" : 1,
                           "numPts" : 4500,
                           "norm" : 50000,
                           "MH" : 0
                           }
                           ]
        },
        
        "run": {
            "algorithm": "NUTS",
            "iter": 3000,
            "chain": 3},
        
        
        "sample": "./examples/MassHierarchy/results/MHtest_analyzer.out",
        
        "output":
        {
            "name" : "./examples/MassHierarchy/results/MHtest_analyzer.h5",
            "format": "hdf5",
            "config": {"group": "/"},
            "fit": "./examples/MassHierarchy/results/MHresults2.pkl",
            "data": [
                     {"stan_parameter": "nu_mass","output_name": "nu_masses"},
                     {"stan_parameter": "th12","output_name": "th12"},
                     {"stan_parameter": "th13","output_name": "th13"},
                     {"stan_parameter": "delta_m21","output_name": "delta_m21"},
                     {"stan_parameter": "delta_m32","output_name": "delta_m32"},
                     {"stan_parameter": "m32_withsign","output_name": "m32_withsign"},
                     {"stan_parameter": "min_mass","output_name": "min_mass"},
                     {"stan_parameter": "mbeta","output_name": "mbeta"},
                     {"stan_parameter": "lp__","output_name": "LogLikelihood"},
                     ]
        }
        
    }
    
}
