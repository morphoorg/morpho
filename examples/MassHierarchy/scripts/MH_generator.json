{
    "stan": {
        
        "name": "MH_generator",
        
        "model":   {
            "file": "./examples/MassHierarchy/models/spectrum_generator.stan",
            "function_file": [
                              {"name" : "func_routines", "file" : "./functions/func_routines.functions"},
                              {"name" : "MH_Functions", "file" : "./functions/MH_functions.functions"}
                              ],
            "cache": "./examples/MassHierarchy/cache"
        },
        
        "data": {
            "type": "mc",
            "files": [
                      {
                      "name": "./examples/MassHierarchy/data/MH_generate.data",
                      "format":"R"
                      }
                      ],
            "parameters": [
                           {
                           "chainnum" : 3,
                           "iternum" : 3000,
                           "nEvents" : 1,
                           "numPts" : 4500,
                           "norm" : 50000,
                           "MH" : 0
                           }
                           ]

        },
        
        "run": {
            "algorithim": "NUTS",
            "iter": 3000,
            "chain": 3},
        
        "plot": [
                 {"variable": "KE"},
                 {"variable": "N"},
                 ],
        
        "sample": "./examples/MassHierarchy/results/MHtest_generator.out",
        
        "output":{
            "name": "./examples/MassHierarchy/results/MHtest_generator",
            "format": "hdf5",
            "fit": "./examples/MassHierarchy/results/MHresults1.pkl",
            "config": {"group": "/"},
            "data": [
                     {"stan_parameter": "KE","output_name": "KEout"},
                     {"stan_parameter": "N","output_name": "Nout"},
                     {"stan_parameter": "rate_log","output_name": "rate_log"},
                     ]
        }
        
    }
    

}