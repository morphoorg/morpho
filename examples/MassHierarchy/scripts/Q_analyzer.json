{
    "stan": {
        
        "name": "Q_analysis",
        
        "model":   {
            "file": "./MassHierarchy/models/Q_analyzer.stan",
            "function_file": [
                              {"name" : "func_routines", "file" : "../functions/func_routines.functions"},
                              {"name" : "constants", "file" : "../functions/constants.functions"}
                              {"name" : "Q_Functions", "file" : "../functions/Q_functions.functions"}
                              ],
            "cache": "./MassHierarchy/cache"
        },

        
        "data": {
            "type": "mc",
            "files": [
                      {
                      "name": "./MassHierarchy/data/Q_analyze.data",
                      "format":"R"
                      },
                      {
                      "name" : "./MassHierarchy/results/Qtest_generator",
                      "format" : "hdf5",
                      "event_number" : 0,
                      "datasets" : [
                                    {"nm" : "Q", "stan_alias" : "Q"},
                                    {"nm" : "sigma", "stan_alias" : "sigma"}
                                    ]
                      }
                      
                      ],
            "parameters": [
                           {
                           "chainnum" : 1,
                           "iternum" : 2000,
                           "numPts" : 4000,
                           }
                           ]
        },
        
        "run": {
            "algorithm": "NUTS",
            "iter": 2000,
            "chain": 1},
        

        "sample": "./MassHierarchy/results/Qtest_analyzer.out",
        
        "output":
        {
            "name" : "./MassHierarchy/results/Qtest_analyzer.h5",
            "format": "hdf5",
            "config": {"group": "/"},
            "fit": "./MassHierarchy/results/Qresults2.pkl",
            "data": [
                     {"stan_parameter": "Q_extract","output_name": "Q_extract"},
                     {"stan_parameter": "sigma_extract","output_name": "sigma_extract"},
                     {"stan_parameter": "lp__","output_name": "LogLikelihood"},
                     ]
        }
        
    }
    
}