{
    "stan": {

        "name": "Q_generator",

        "model":   {
            "file": "./examples/MassHierarchy/models/Q_generator.stan",
            "function_file": [
                {"name" : "constants", "file" : "../functions/constants.functions"},
                {"name" : "func_routines", "file" : "../functions/func_routines.functions"},
                {"name" : "Q_Functions", "file" : "../functions/Q_functions.functions"}
             ],
            "cache": "./examples/MassHierarchy/cache"
        },

        "data": {
            "type": "mc",
            "files": [
                      {
                      "name": "./MassHierarchy/data/Q_generate.data",
                      "format":"R"
                      }
                      ]
        },

        "run": {
            "algorithm": "NUTS",
            "iter": 10000,
            "chain": 5},
        
        "sample": "./MassHierarchy/results/Qtest_generator_T.out",
        
        "output": 
            {
            "name" : "./MassHierarchy/results/Qtest_generator_T",
            "format": "hdf5",
            "fit": "./MassHierarchy/results/Qresults_T.pkl",
            "config": {"group": "/"},
            "data": [
                     {"stan_parameter": "Q","output_name": "Q"},
                     {"stan_parameter" : "epsilon", "output_name" : "epsilon"},
                     {"stan_parameter" : "kappa", "output_name" : "kappa"},
                     {"stan_parameter" : "eta", "output_name" : "eta"},
                     {"stan_parameter" : "sigma_avg", "output_name" : "sigma_avg"},
                     {"stan_parameter" : "temperature", "output_name" : "temperature"},
                     {"stan_parameter" : "composition", "output_name" : "composition", "ndim":4},
                     {"stan_parameter" : "sigma", "output_name" : "sigma", "ndim":4},
                     {"stan_parameter" : "lp__", "output_name" : "LogLikelihood"}
                     ]
             }        

   }


}
