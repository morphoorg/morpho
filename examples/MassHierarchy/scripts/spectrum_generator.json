{
    "stan": {
        
        "name": "spectrum_generator",
        
        "model":   {
            "file": "./examples/MassHierarchy/models/spectrum_generator.stan",
            "function_file": [
                              {"name" : "func_routines", "file" : "./functions/func_routines.functions"},
                              {"name" : "MH_Functions", "file" : "./functions/MH_functions.functions"}
                              ],
            "cache": "./examples/MassHierarchy/cache"
        },
        
        "data": {
            "type": "mc",
            "files": [
                      {
                      "name": "./examples/MassHierarchy/data/MH_generate.data",
                      "format":"R"
                      }
                      ],
            "parameters": [
                           {
                           "nChains" : 4,
                           "nGenerate" : -500,
                           "MH" : 0
                           }
                           ]

        },
        
        "run": {
            "algorithim": "NUTS",
            "iter": 1000,
            "warmup": 800,
            "chain": 4},
        
        "plot": [
                 {"variable": "KE_data"},
                 {"variable": "spectrum_data"},
                 ],
        
        "sample": "./examples/MassHierarchy/results/MHtest_generator.out",
        
        "output":{
            "name": "./examples/MassHierarchy/results/MHtest_generator.root",
            "format": "root",
            "tree" : "generated",
            "branches" : [
                    {"variable" : "KE_data", "root_alias" : "KE_data"},
                    {"variable" : "spectrum_data", "root_alias" : "spectrum_data"},
                    {"variable" : "nData", "root_alias" : "nData", "type" : "int"}
                     ]
        }
        
    }
    

}
