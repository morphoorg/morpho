{
  "stan": {

      "name": "krypton_xsec_generator",

    "model":   {
      "file": "./krypton_run/models/krypton_xsec_generator.stan",
      "cache": "./krypton_run/cache"
      },

    "data": {
       "type": "mc",
      "files": [
          {
         "name": "./krypton_run/data/krypton_run.header.data", 
         "format":"R"
          },
          {
         "name": "./krypton_run/data/krypton_monte_carlo.data", 
         "format":"R"
          }],
      "parameters": [
          {
         "BField" : 0.95338,
         "TrapCurrent" : 400,
         "minFreq" : 25730000000.0,
         "maxFreq" : 25800000000.0,
         "xsec_avekin" : 2.568, 
         "xsec_bindkin" : 21.0,
         "xsec_msq" : 1.598,
         "xsec_Q" : 1.173,
         "nPar":  2,
         "eLossRatio": [0.8, 0.2],
         "eLossMean":  [12.60, 14.3],
         "eLossWidth":  [1.85, 12.5]
          }
         ]
   },

   "run": {
          "algorithim": "NUTS",
          "iter": 10000,
          "warmup": 5000,
          "chain": 1},

   "plot": [
      {"variable": "TotalField"},
      {"variable": "KE"},
      {"variable": "frequency"}
      ],

   "sample": "./krypton_run/results/test_xsec_generator.out",

   "output": 
        {
         "name" : "./krypton_run/results/test_xsec_generator",
         "format": "root",
         "tree" : "stanAnalysis",
         "branches" : [
               {"variable" : "KE", "root_alias" : "KE"},
               {"variable" : "beta", "root_alias" : "beta"},
               {"variable" : "stheta", "root_alias" : "stheta"},
               {"variable" : "frequency", "root_alias" : "frequency"},
               {"variable" : "xsec", "root_alias" : "xsec"},
               {"variable" : "scattering_length", "root_alias" : "scattering_length"},
               {"variable" : "eLoss", "root_alias" : "eLoss"},
               {"variable" : "freq_data", "root_alias" : "freq_data"},
               {"variable" : "dfdt_data", "root_alias" : "dfdt_data"},
               {"variable" : "time_data", "root_alias" : "time_data"},
               {"variable" : "freq_jump_data", "root_alias" : "freq_jump_data"},
               {"variable" : "dfdt_jump_data", "root_alias" : "dfdt_jump_data"},
               {"variable" : "lp__", "root_alias" : "LogLikelihood"}
                ]
         }      

   }

}
